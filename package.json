{
  "name": "transitlog-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.5",
    "apollo-client": "^2.4.2",
    "apollo-link-http": "^1.5.5",
    "customize-cra": "^0.1.0-beta5",
    "date-fns": "^1.29.0",
    "forever": "^0.15.3",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "leaflet": "^1.3.4",
    "localforage": "^1.7.2",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.49.0",
    "mapillary-js": "^2.13.0",
    "mobx": "^5.5.0",
    "mobx-app": "^1.0.3",
    "mobx-react": "^5.2.8",
    "mobx-utils": "^5.0.2",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "p-queue": "^3.0.0",
    "polished": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.5.2",
    "react-apollo": "^2.2.3",
    "react-app-rewired": "^1.6.2",
    "react-autosuggest": "^9.4.2",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-leaflet": "^2.0.0",
    "react-primitives-svg": "^0.0.3",
    "react-scripts": "^2.0.4",
    "serve": "^10.0.2",
    "styled-components": "^3.4.9"
  },
  "scripts": {
    "docker:build": "docker build -t hsldevcom/transitlog-ui .",
    "docker:run": "docker run -d -p 3000:3000 hsldevcom/transitlog-ui",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "CI=true react-app-rewired test",
    "test:staged": "CI=true react-app-rewired test --bail --findRelatedTests",
    "eject": "react-app-rewired eject",
    "codestyle": "prettier \"src/**/*.{css,js}\" --write",
    "lint": "eslint \"src/**/*.js\" --fix",
    "production": "forever start -c \"yarn run serve -s -l 3000\" build && forever logs -f 0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2",
    "react-testing-library": "^5.1.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "npm run test:staged --"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
