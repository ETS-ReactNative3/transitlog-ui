{
  "name": "transitlog-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link-http": "^1.5.9",
    "date-fns": "^1.30.1",
    "forever": "^0.15.3",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "history": "^4.7.2",
    "leaflet": "^1.4.0",
    "leaflet-draw": "^1.0.4",
    "localforage": "^1.7.3",
    "localforage-indexes": "^1.1.1",
    "localforage-lru-driver": "^1.0.4",
    "localforage-removeitems": "^1.4.0",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.52.0",
    "mapillary-js": "^2.14.1",
    "mobx": "^5.8.0",
    "mobx-app": "^1.0.3",
    "mobx-react": "^5.4.3",
    "mobx-utils": "^5.1.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "p-all": "^1.0.0",
    "p-each-series": "^1.0.0",
    "p-finally": "^1.0.0",
    "p-map": "^2.0.0",
    "polished": "^2.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-autosuggest": "^9.4.2",
    "react-datepicker": "^2.0.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-leaflet": "^2.1.4",
    "react-leaflet-draw": "^0.19.0",
    "react-primitives-svg": "^0.0.3",
    "react-scripts": "^2.1.2",
    "serve": "^10.1.1",
    "styled-components": "^4.1.3",
    "styled-react-modal": "^1.2.1"
  },
  "scripts": {
    "docker:build": "docker build -t hsldevcom/transitlog-ui .",
    "docker:run": "docker run -d -p 3000:3000 hsldevcom/transitlog-ui",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "test:staged": "CI=true react-scripts test --bail --findRelatedTests",
    "eject": "react-scripts eject",
    "codestyle": "prettier \"src/**/*.{css,js}\" --write",
    "lint": "eslint \"src/**/*.js\" --fix",
    "production": "forever start -c \"yarn run serve -s -l 3000\" build && forever logs -f 0",
    "prepare": "patch-package"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mobx-react-devtools": "^6.0.3",
    "patch-package": "^6.0.0-9",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0",
    "react-testing-library": "^5.4.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add",
      "npm run test:staged --"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
